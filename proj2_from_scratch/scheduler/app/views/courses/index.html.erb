
<div class="container">
  <div class="row">
    <h2>Browse by Department</h2>
    <div class="column">
      <table class="table">
        <tbody>
          <% @courses.each do |c| %>
            <tr>
              <%= c.id %>: <%= c.title %>: <%= c.course_name %>
              <br>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
