
<div class="container">
  <div class="row">
    <h2>All Schedules</h2>
    <div class="column">
      <table class="table">
        <tbody>
          <% @schedules.each do |s| %>
            <tr>
              <% if s.email == current_student.email %>
                <p><%= button_to s.name, schedule_show_path(id: s.id), :class => "button medium", :method => :get %></p>
              <% end %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
